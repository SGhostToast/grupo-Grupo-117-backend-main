  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.1.3",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa2-swagger-ui": "^5.8.0",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "postgresql": "^0.0.1",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.0",
    "sequelize-noupdate-attributes": "^1.0.0",
    "yamljs": "^0.3.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "model": "sequelize-cli seed:model --name",
    "seed": "sequelize-cli seed:generate --name",
    "plant": "sequelize-cli db:seed",
    "plantall": "sequelize-cli db:seed:all",
    "unpot": "sequelize-cli db:seed:undo",
    "unpotall": "sequelize-cli db:seed:undo:all",
    "migrate": "sequelize-cli db:migrate",
    "unmigrate": "sequelize-cli db:migrate:undo",
    "build": "sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "destroy": "sequelize-cli db:migrate:undo:all",
    "reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "devDependencies": {
    "eslint": "^8.41.0"
  }
}
